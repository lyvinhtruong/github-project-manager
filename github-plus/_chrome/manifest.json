{
  // Required
  "manifest_version": 2,
  "name": "GitHub+",
  "version": "0.9.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Adds extra features to GitHub issues.",
  "author": "Bob Lauer",
  "content_scripts": [{
    "matches": ["https://github.com/*/*/issues/*", "https://github.com/*/*/pull/*"],
    "js": ["js/_inject.js"],
    "css": ["css/ghplus.css"],
    "run_at": "document_end"
  }],
  "homepage_url": "https://github.com/boblauer/GitHubPlus",
  "web_accessible_resources": [
    "js/ghplus.js",
    "css/images/animated-overlay.gif",
    "css/images/ui-bg_diagonals-thick_18_b81900_40x40.png",
    "css/images/ui-bg_diagonals-thick_20_666666_40x40.png",
    "css/images/ui-bg_flat_10_000000_40x100.png",
    "css/images/ui-bg_flat_35_4183c4_40x100.png",
    "css/images/ui-bg_glass_100_f6f6f6_1x400.png",
    "css/images/ui-bg_highlight-soft_100_146fc9_1x100.png",
    "css/images/ui-bg_highlight-soft_100_eeeeee_1x100.png",
    "css/images/ui-bg_highlight-soft_75_f6f6f6_1x100.png",
    "css/images/ui-icons_222222_256x240.png",
    "css/images/ui-icons_228ef1_256x240.png",
    "css/images/ui-icons_ef8c08_256x240.png",
    "css/images/ui-icons_ffd27a_256x240.png",
    "css/images/ui-icons_ffffff_256x240.png"
  ]
}
